<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 64 64">
  <!-- Background -->
  <rect width="64" height="64" rx="16" fill="#0a0e17"/>
  
  <!-- Quantum Particle Effect -->
  <circle cx="32" cy="32" r="16" fill="none" stroke="#00ccff" stroke-width="2" opacity="0.7">
    <animate attributeName="r" values="16;18;16" dur="4s" repeatCount="indefinite"/>
    <animate attributeName="opacity" values="0.7;0.9;0.7" dur="4s" repeatCount="indefinite"/>
  </circle>
  
  <!-- Electron Orbits -->
  <ellipse cx="32" cy="32" rx="24" ry="14" fill="none" stroke="#7b42f6" stroke-width="1.5" opacity="0.5" transform="rotate(30, 32, 32)">
    <animate attributeName="rx" values="24;26;24" dur="5s" repeatCount="indefinite"/>
    <animate attributeName="ry" values="14;16;14" dur="5s" repeatCount="indefinite"/>
  </ellipse>
  
  <ellipse cx="32" cy="32" rx="20" ry="24" fill="none" stroke="#00ccff" stroke-width="1.5" opacity="0.5" transform="rotate(60, 32, 32)">
    <animate attributeName="rx" values="20;22;20" dur="6s" repeatCount="indefinite"/>
    <animate attributeName="ry" values="24;26;24" dur="6s" repeatCount="indefinite"/>
  </ellipse>
  
  <!-- Center Nucleus -->
  <circle cx="32" cy="32" r="6" fill="#7b42f6">
    <animate attributeName="r" values="6;7;6" dur="3s" repeatCount="indefinite"/>
  </circle>
  <circle cx="32" cy="32" r="3" fill="#00ccff"/>
</svg>
